{
    "name": "saft/saft-cache-phparray",
    "type": "library",
    "description": "PHP-Array cache adapter from the library Saft.",
    "keywords": ["Saft", "Cache", "PHPArray"],
    "homepage": "https://github.com/SaftIng/Saft.cache.phparray",
    "license": "GPL2",
    "require": {
        "php": ">5.3.3",

        "saft/saft-cache": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "symfony/yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "Saft\\": "src/Saft/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saft\\": [
                "test/integration/Saft",
                "test/unit/Saft"
            ]
        }
    },
    "minimum-stability":"dev"
}
