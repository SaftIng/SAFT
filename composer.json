{
    "name": "saft/saft-cache-file",
    "type": "library",
    "description": "File cache adapter from the library Saft.",
    "keywords": ["Saft", "Cache", "File"],
    "homepage": "https://github.com/SaftIng/Saft.cache.file",
    "license": "GPL2",
    "authors": [
        {
            "name": "Natanael Arndt",
            "homepage": "http://aksw.org/NatanaelArndt",
            "email": "arndtn@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Konrad Abicht",
            "homepage": "http://www.inspirito.de",
            "email": "hi@inspirito.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">5.3.3",
        
        "saft/saft-cache": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "symfony/yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "Saft\\": "src/Saft/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saft\\": [
                "test/integration/Saft",
                "test/unit/Saft"
            ]
        }
    },
    "minimum-stability":"dev"
}
