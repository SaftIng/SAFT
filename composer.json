{
    "name": "saft/saft",
    "type": "library",
    "description": "Saft library provides tools and an infrastructure to build Semantic Web and Linked Data applications.",
    "keywords": ["Saft", "Linked Data", "Semantic Web", "RDF", "Triple Store", "Parser"],
    "homepage": "https://safting.github.io/",
    "license": "GPL2",
    "authors": [
        {
            "name": "Natanael Arndt",
            "homepage": "http://aksw.org/NatanaelArndt",
            "email": "arndtn@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Konrad Abicht",
            "homepage": "http://www.inspirito.de",
            "email": "hi@inspirito.de",
            "role": "Developer"
        },
        {
            "name": "Viktor Befort",
            "email": "viktor.befort@stud.htwk-leipzig.de",
            "role": "Developer"
        }
    ],
    "repositories" : [
        {
            "type": "vcs",
            "url": "https://github.com/discordier/php-filesystem.git"
        }
    ],
    "replace": {
        "saft/saft-rdf": "self.version",
        "saft/saft-store": "self.version",
        "saft/saft-data": "self.version",
        "saft/saft-sparql": "self.version",
        "saft/saft-cache": "self.version",
        "saft/saft-cache-file": "self.version",
        "saft/saft-cache-phparray": "self.version",
        "saft/saft-cache-memcached": "self.version",
        "saft/saft-querycache": "self.version",
        "saft/saft-store-http": "self.version",
        "saft/saft-store-virtuoso": "self.version",
        "saft/saft-store-local": "self.version"
    },
    "require": {
        "php": ">=5.3.3",
        "php-curl-class/php-curl-class": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "phpunit/php-code-coverage": "2.0.15",
        "squizlabs/php_codesniffer": "dev-master",
        "sami/sami": "2.*@dev",
        "monolog/monolog": "1.11.*",
        "filicious/filicious" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Saft\\": [
                "src/Saft/"
            ]
        }
    }
}
