{
    "description": "Redland backend for the Saft library.",
    "keywords": ["Saft", "Linked Data", "Semantic Web", "Redland"],
    "homepage": "https://safting.github.io",
    "license": "GPL2",
    "name": "saft/saft-redland",
    "require": {
        "php": ">=5.3.3",
        "ext-redland": "*",

        "saft/saft-data": "dev-master",
        "saft/saft-rdf": "dev-master",
        "saft/saft-store": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Saft\\Backend\\Redland\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saft\\Backend\\Redland\\Tests\\": [
                "test/"
            ]
        }
    }
}
